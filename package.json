{
  "name": "term-project-hearts-game",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "NODE_ENV=development DEBUG=APP_NAME:* nodemon ./bin/www",
    "db:create:migration": "sequelize migration:generate --name ",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "postinstall": "sequelize db:migrate",
    "db:create:seed": "sequelize seed:generate --name ",
    "db:seed:all": "sequelize db:seed:all",
    "db:seed": "sequelize db:seed --seed ",
    "db:seed:undo": "sequelize db:seed:undo:all"
  },
  "dependencies": {
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "clean-css": "^4.2.3",
    "cookie-parser": "^1.4.5",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "event-stream": "^3.3.5",
    "express": "~4.16.0",
    "express-session": "^1.17.1",
    "express-validator": "^5.3.1",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg-promise": "^8.7.5",
    "pug": "^2.0.4",
    "save": "^2.4.0",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^5.5.1",
    "set-value": "^3.0.2",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
